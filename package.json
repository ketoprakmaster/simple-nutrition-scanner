{
	"name": "svelte-latest",
	"private": true,
	"version": "1.1.5-pwa",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"preview:serve": "cross-env BASE_PATH='/myapp' && bun run build && bun run preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"generate-pwa-assets": "pwa-assets-generator --preset minimal-2023 static/favicon.svg"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.18",
		"@types/bun": "^1.3.9",
		"@vite-pwa/assets-generator": "^1.0.2",
		"cross-env": "^10.1.0",
		"daisyui": "^5.5.17",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@ericblade/quagga2": "^1.12.1",
		"dexie": "^4.3.0",
		"workbox-core": "^7.4.0",
		"workbox-expiration": "^7.4.0",
		"workbox-precaching": "^7.4.0"
	}
}
